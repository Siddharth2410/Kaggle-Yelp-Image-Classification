osboxes@osboxes:~$ mogrify -resize '256x256' caffedemotrain/*

copy top n files
osboxes@osboxes:~$ find ./caffedemotrain -maxdepth 1 -type f |head -70|xargs mv -t ./caffedemoval


copy 
